{
  "name": "sn-rrule-generator",
  "version": "1.0.0",
  "description": "ServiceNow UI Page that can generate rrules",
  "scripts": {
    "dev": "parcel ./src/index.html",
    "parcel-build": "parcel build ./src/index.html --reporter @parcel/reporter-bundle-analyzer --no-source-maps",
    "sn-build": "node ./script/sn-build.js",
    "build": "rimraf ./dist && mkdirp ./dist && npm run parcel-build && npm run sn-build",
    "deploy": "npm run build && node ./script/sn-deploy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mak1A4/sn-rrule-generator.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mak1A4/sn-rrule-generator/issues"
  },
  "homepage": "https://github.com/mak1A4/sn-rrule-generator#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-rrule-generator": "^1.2.0",
    "rrule": "^2.6.8"
  },
  "devDependencies": {
    "@parcel/reporter-bundle-analyzer": "^2.0.0-nightly.1694",
    "@parcel/transformer-sass": "^2.0.0-beta.3.1",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "cheerio": "^1.0.0-rc.10",
    "mime-types": "^2.1.31",
    "mkdirp": "^1.0.4",
    "parcel": "^2.0.0-beta.3.1",
    "rimraf": "^3.0.2",
    "sn-oauth": "github:mak1A4/sn-oauth",
    "yaml": "^1.10.2"
  }
}
